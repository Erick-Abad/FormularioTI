{
  "version": 2,
  "builds": [
    {
      "src": "src/api/sendEmail.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/sendEmail",
      "dest": "/src/api/sendEmail.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
